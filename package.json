{
  "name": "crop-calendar",
  "version": "1.0.0",
  "description": "Agricultural advisory system with crop calendar and satellite data integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd backend && npm run dev",
    "client:dev": "cd frontend && npm start",
    "server:start": "cd backend && npm start",
    "client:build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "npm run client:build",
    "start": "npm run server:start"
  },
  "keywords": ["agriculture", "crop-calendar", "satellite-data", "farming"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
